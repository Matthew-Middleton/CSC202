/**This program is designed to log the keys of a keyboard.
 * Algorithm:
 */
package keylogger;
import java.io.*;
@SuppressWarnings("unused")
public class KeyLogger
{
	private Data data;
	private ObjectOutputStream outStream = null;
	private ObjectInputStream inStream = null;
	private File file = new File("keysLogged.dat");
	
	public KeyLogger()
	{
		
	}
	
	public KeyLogger(Data keysLogged)
	{
		this.data = keysLogged;
	}
	/**Logs keys and translates them into a stream to store in a binary data file
	 * @param data
	 */
	//need to think of how to treat char data and convert into binary
	private void takeStream(Data data)
	{
		try
		{
			outStream = new ObjectOutputStream(new FileOutputStream("keysLogged.dat"));
		}
		catch(IOException ioe)
		{
			
		}
	}
	/**Sets the data to be input into a stream
	 * @param keysLogged
	 */
	public void setData(Data keysLogged)
	{
		this.data = keysLogged;
	}
	/**Returns
	 * @return
	 */
	public Data getData()
	{
		return data;
	}
}
