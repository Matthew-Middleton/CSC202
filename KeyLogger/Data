/**Object to store the keys that have been logged.
 * Data stored is of type char
 * 
 */
package keylogger;

public class Data
{
	private char data;
	
	public Data()
	{
		
	}
	
	public Data(char keysLogged)
	{
		this.data = keysLogged;
	}
	/**Sets the data for the keys logged
	 * @param keysLogged
	 */
	public void setData(char keysLogged)
	{
		this.data = keysLogged;
	}
	/**Returns the data for the keys logged
	 * @return
	 */
	public char getData()
	{
		return data;
	}
}
